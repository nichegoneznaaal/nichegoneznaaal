   "source": [
    "def almost_double_factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    a = 1\n",
    "    for i in range(1,n+1,2):\n",
    "        a = a * i\n",
    "    return a\n",
    "\n"
   ]
